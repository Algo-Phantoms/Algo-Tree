#include <bits/stdc++.h>
using namespace std;
 
void kLargest(int arr[], int n, int k)
{
    
    sort(arr, arr + n, greater<int>());
 
    
    for (int i = 0; i < k; i++)
        cout << arr[i] << " ";
}
 

int main()
{   
    int n , k;
    int arr[n];
    cout << "Enter Array length" << endl;
    cin >> n ;
    cout << "Enter Array Elements" << endl ;
    
    for(int i = 0 ; i < n ; i++)
    {
       cin >> arr[i];  
    }
    
    cout << "Enter k value" << endl;
    cin >> k ;
    kLargest(arr, n, k);
    
    return 0;
}
